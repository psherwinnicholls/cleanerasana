{
  "short_name": "Cleaner Asana",
  "name": "Cleaner Asana",
  "manifest_version": 2,
  "version": "0.1.0",
  "browser_action":{
    "default_popup":"index.html"
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "content_scripts": [{
    "js": ["main.js"],
    "matches": ["https://app.asana.com/*"]
  }],
  "permissions": [
   "storage"
 ]
}
